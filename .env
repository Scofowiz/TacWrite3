# AI Provider Configuration
# Set VITE_AI_PROVIDER to: mock, gemini, cloudflare, production, or groq
VITE_AI_PROVIDER=gemini

# Model Configuration
# Gemini Models: gemini-2.0-flash, gemini-3-pro-preview, gemini-1.5-pro
# Groq Model: moonshotai/kimi-k2-instruct-0905
GEMINI_MODEL=gemini-3-pro-preview
GROQ_MODEL=moonshotai/kimi-k2-instruct-0905

# Gemini AI Configuration
GEMINI_API_KEY=your_gemini_api_key_here
VITE_GEMINI_API_KEY=your_gemini_api_key_here
VITE_GEMINI_MODEL=gemini-3-pro-preview

# Cloudflare Workers AI Configuration
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here
VITE_CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here
VITE_CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here
VITE_CLOUDFLARE_MODEL=@cf/meta/llama-3.1-8b-instruct

# Groq AI Configuration
GROQ_API_KEY=your_groq_api_key_here
VITE_GROQ_API_KEY=your_groq_api_key_here
VITE_GROQ_MODEL=moonshotai/kimi-k2-instruct-0905

# Legacy AI Configuration
VITE_AI_API_KEY=your_legacy_api_key_here
VITE_AI_MODEL=gemini-3-pro-preview

# Database Configuration
DATABASE_URL=postgresql://scottfoster@localhost:5432/tacwrite3_dev

# Server Configuration
PORT=3000
NODE_ENV=development
